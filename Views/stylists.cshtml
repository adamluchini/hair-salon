<head>
  <meta charset="utf-8">
</head>
<body>
<h1>All Stylists</h1>
@if (@Model.Count == 0)
{
  <p>No stylists yet</p>
}
<ul>
  @foreach (var stylist in Model)
  {
  <li><a href = "/stylists/@stylist.GetId()">@stylist.GetName()</a></li>
  }
</ul>
<br><br>
<a href = "stylists/new">Add new stylist</a>
<p><a href = "/">Return home</a></p>
</body>
